<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="52dp"
    android:layout_margin="02dp"
    android:elevation="@dimen/card_elevation_default">


    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat8"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/it_sequencial"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.6"
            android:elevation="20dp"
            android:layout_gravity="center_vertical"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:text="2" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/it_num_serie"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:elevation="20dp"
            android:layout_gravity="center_vertical"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:text="P23279474973494" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/it_sku"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:elevation="20dp"
            android:layout_gravity="center_vertical"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:text="3374979433.34343.34343.34" />


    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        style="@style/view_bottom_default"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>