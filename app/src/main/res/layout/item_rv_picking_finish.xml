<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:foreground="?selectableItemBackground">


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_descricaoDistribuicao_picking3"
        style="@style/txt_dados_rv_etc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_weight="1"
        android:text="Distribuição"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appCompatImageView7" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/api_descricaoDistribuicao_picking3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="end"
        app:layout_constraintEnd_toEndOf="@+id/appCompatImageView7"
        app:layout_constraintTop_toTopOf="@+id/txt_descricaoDistribuicao_picking3"
        tools:text="Distribuiçao 1" />


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_descricaoEmbalagem_picking3"
        style="@style/txt_dados_rv_etc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_weight="1"
        android:text="Embalagem"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_descricaoDistribuicao_picking3" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/api_descricaoEmbalagem_picking3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="end"
        app:layout_constraintEnd_toEndOf="@+id/appCompatImageView7"
        app:layout_constraintTop_toBottomOf="@+id/api_descricaoDistribuicao_picking3"
        tools:text="Corrugado 12 PRS" />


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_sku_picking3"
        style="@style/txt_dados_rv_etc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_weight="1"
        android:text="Sku"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_descricaoEmbalagem_picking3" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/api_sku_picking3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="end"
        app:layout_constraintEnd_toEndOf="@+id/appCompatImageView7"
        app:layout_constraintTop_toBottomOf="@+id/api_descricaoEmbalagem_picking3"
        tools:text="40109.2839.23893792.23723923" />


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_quantidade_picking3"
        style="@style/txt_dados_rv_etc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="16dp"
        android:layout_weight="1"
        android:text="Quantidade"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_sku_picking3" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/api_quantidade_picking3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="end"
        app:layout_constraintEnd_toEndOf="@+id/appCompatImageView7"
        app:layout_constraintTop_toBottomOf="@+id/api_sku_picking3"
        tools:text="1" />


    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/appCompatImageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/ic_next_24_card"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        style="@style/view_bottom_default"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>