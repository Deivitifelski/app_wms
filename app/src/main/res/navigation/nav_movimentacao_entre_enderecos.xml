<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_movimentacao_entre_enderecos"
    app:startDestination="@id/returnTaskFragment12">

    <fragment
        android:id="@+id/returnTaskFragment12"
        android:name="com.documentos.wms_beirario.ui.movimentacaoentreenderecos.fragments.ReturnTaskFragment1"
        android:label="ReturnTaskFragment1"
        tools:layout="@layout/fragment_return_task1" >
        <action
            android:id="@+id/action_returnTaskFragment12_to_endMovementFragment2"
            app:destination="@id/endMovementFragment2" />
    </fragment>
    <fragment
        android:id="@+id/endMovementFragment2"
        android:name="com.documentos.wms_beirario.ui.movimentacaoentreenderecos.fragments.EndMovementFragment2"
        android:label="EndMovementFragment2"
        tools:layout="@layout/fragment_end_movement2" >
        <action
            android:id="@+id/action_endMovementFragment2_to_returnTaskFragment12"
            app:destination="@id/returnTaskFragment12" />
        <argument
            android:name="itemClickedMov1"
            app:argType="com.documentos.wms_beirario.model.movimentacaoentreenderecos.MovementResponseModel1"
            app:nullable="true" />
        <argument
            android:name="idNewTarefa"
            app:argType="com.documentos.wms_beirario.model.movimentacaoentreenderecos.MovementNewTask"
            app:nullable="true" />

    </fragment>
</navigation>